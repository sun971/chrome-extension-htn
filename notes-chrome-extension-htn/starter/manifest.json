{ 
  "name": "Note Taker App Starter",
  "version": "0.0.1",
  "description": "Take Notes on web pages!",
  "manifest_version": 2,
  "background":{
    "scripts":["background.js"],
    "persistent":false
  },
  "page_action":{
    "default_popup": "popup/popup.html"
  
  },
  "permissions":["tabs","activeTab", "declarativeContent","storage"]
}